    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.3",
