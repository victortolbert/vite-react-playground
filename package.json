    "million": "3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.3",
    "@shopify/polaris": "^13.1.0",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@eslint-react/eslint-plugin": "^1.5.8",
